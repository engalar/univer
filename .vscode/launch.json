    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Doc (Chrome)",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3003",
            "webRoot": "${workspaceFolder}/examples/docs/local/",
            "presentation": {
                "group": "0_example",
                "order": 1
            }
        },
        {
            "name": "Launch Doc (Edge)",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3003",
            "webRoot": "${workspaceFolder}/examples/docs/local/",
            "presentation": {
                "group": "0_example",
                "order": 2
            }
        },
        {
            "name": "Launch Sheet (Chrome)",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3002",
            "webRoot": "${workspaceFolder}/examples/sheets/local/",
            "presentation": {
                "group": "0_example",
                "order": 3
            }
        },
        {
            "name": "Launch Sheet (Edge)",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3002",
            "webRoot": "${workspaceFolder}/examples/sheets/local/",
            "presentation": {
                "group": "0_example",
                "order": 4
            }
        },
        {
            "name": "Launch Mendix (Chrome)",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3002",
            "webRoot": "${workspaceFolder}/examples/mendix/local/",
            "presentation": {
                "group": "0_example",
                "order": 7
            }
        },
        {
            "name": "Launch Mendix (Edge)",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3002",
            "webRoot": "${workspaceFolder}/examples/mendix/local/",
            "presentation": {
                "group": "0_example",
                "order": 8
            }
        },
        {
            "name": "Launch Slide (Chrome)",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3004",
            "webRoot": "${workspaceFolder}/examples/slides/local/",
            "presentation": {
                "group": "0_example",
                "order": 5
            }
        },
        {
            "name": "Launch Slide (Edge)",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3004",
            "webRoot": "${workspaceFolder}/examples/slides/local/",
            "presentation": {
                "group": "0_example",
                "order": 6
            }
        },
        {
            "type": "node",
            "name": "vscode-jest-tests",
            "request": "launch",
            "args": [
                "--runInBand",
                "--watchAll=false"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true,
            "program": "${workspaceFolder}/node_modules/.bin/jest",
            "windows": {
                "program": "${workspaceFolder}/node_modules/jest/bin/jest"
            }
        },
        {
            "type": "pwa-msedge",
            "name": "Launch Microsoft Edge",
            "request": "launch",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ],
            "url": "d:\\Users\\eg\\.vscode\\extensions\\ms-edgedevtools.vscode-edge-devtools-2.1.5\\out\\startpage\\index.html", // Provide your project's url to finish configuring
            "presentation": {
                "hidden": true
            }
        },
        {
            "type": "pwa-msedge",
            "name": "Launch Microsoft Edge in headless mode",
            "request": "launch",
            "runtimeArgs": [
                "--headless",
                "--remote-debugging-port=9222"
            ],
            "url": "http://localhost:3002", // Provide your project's url to finish configuring
            "presentation": {
                "hidden": true
            }
        },
        {
            "type": "vscode-edge-devtools.debug",
            "name": "Open Edge DevTools",
            "request": "attach",
            "url": "http://localhost:3002", // Provide your project's url to finish configuring
            "presentation": {
                "hidden": true
            }
        }
    ],
    "compounds": [
        {
            "name": "Launch Edge Headless and attach DevTools",
            "configurations": [
                "Launch Microsoft Edge in headless mode",
                "Open Edge DevTools"
            ]
        },
        {
            "name": "Launch Edge and attach DevTools",
            "configurations": [
                "Launch Microsoft Edge",
                "Open Edge DevTools"
            ]
        }
    ]
}
